    "@open-wc/storybook-prebuilt": "^0.0.2",
    "@storybook/addon-docs": "^5.2.5",
    "es-dev-server": "^1.23.1",
    "glob": "^7.1.3"
  },
  "devDependencies": {
    "babel-loader": "^8.0.0",
