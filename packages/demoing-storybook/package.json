{
  "name": "@open-wc/demoing-storybook",
  "version": "0.4.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "Storybook configuration following open-wc recommendations",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-wc/open-wc.git",
    "directory": "packages/building-storybook"
  },
  "author": "open-wc",
  "homepage": "https://github.com/open-wc/open-wc/",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "../../scripts/insert-header.js",
    "site:buildxxxx": "build-storybook -c demo/.storybook -o ../../_site/demoing-storybook",
